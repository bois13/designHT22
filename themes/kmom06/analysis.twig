{% include 'incl/header.twig' %}

<body>
    {% include 'incl/nav.twig' %}

    <div class="main" role="main">
        <div class="landingpage analysis">
            <h1>{{ meta.title }}</h1>

                {% for page in pages("analysis") if not page.hidden %}
                    <div class="{{ page.title|lower }}">
                        <a href="{{ page.url }}">
                            <h2>{{ page.title }}</h2>
                        </a>
                    </div>
                {% endfor %}
        </div>
        <section>
            {{ content }}
        </section>
    </div>
    
    {% include 'incl/footer.twig' %}
</body>

</html>
